<% @planets.each do |planet| %>
	<h3><%= planet.name %></h3>
	<a href="/planets/<%= planet.id %>">
		<img src="<%= planet.image_url %>" alt="">
	</a>
	<ul>
		<li>Orbit Duration: <%= planet.orbit_duration %></li>
		<li>Diameter: <%= planet.diameter %></li>
		<li>Mass: <%= planet.mass %></li>
		<li>Number of Moons: <%= planet.moons %></li>
	</ul>
	<a href="/planets/destroy/<%= planet.id %>">delete</a>

<% end %>